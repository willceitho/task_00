[{{ app_package_name }}-{{ app_repo_name }}]
name={{ app_package_name }} repository for {{ app_repo_name }} packages
baseurl={{ app_repo_base }}/packages/{{ app_repo_name }}/yum
gpgcheck=1
gpgkey={{ app_repo_base }}/GPG-KEY-{{ app_package_name }}
enabled=1
autorefresh=1
type=rpm-md
